{
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "target": "esnext",
        "skipLibCheck": true
    },
    "include": [
        "functions/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}